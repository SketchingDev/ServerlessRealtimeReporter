    "@types/jest": "^24.0.22",
    "@types/node": "^10.12.18",
    "@types/p-retry": "^3.0.1",
    "@types/uuid": "^3.4.6",
    "async-retry": "^1.2.3",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^24.9.0",
    "p-retry": "^4.2.0",
    "serverless": "^1.57.0",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "uuid": "^3.3.3",
